    <style>
        .tittle{
            color: white;
        }
        .sidebar {
            background-color: #181E4B;
            color: white;
            padding: 20px;
            text-align: center;
        }
        .sidebar img {
            border-radius: 50%;
            width: 150px;
            height: 150px;
            object-fit: cover;
        }
        .main-content {
            padding: 20px;
        }
        .section-title {
            font-size: 1.25rem;
            margin-bottom: 10px;
            border-bottom: 2px solid #181E4B;
            padding-bottom: 5px;
        }
        .skill-item, .project-item {
